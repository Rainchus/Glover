{
    "version": 4,
    "configurations": [
        {
            "name": "glover",
            "compilerPath": "/usr/bin/gcc",
            "compilerArgs": [
                "-m32",
                "-I.",
                "-I include",
                "-I include/PR",
                "-I build/include",
                "-I src"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "${workspaceFolder}/include/**",
                    "${workspaceFolder}/src/**"
                ]
            },
            "includePath": [
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/src/**",
                "/usr/include/x86_64-linux-gnu"
            ],
            "defines": [
                "F3DEX_GBI_2",
                "_LANGUAGE_C",
                "_MIPS_SZLONG=32"
            ],
            "cStandard": "gnu89",
            "intelliSenseMode": "${default}"
        }
    ]
}